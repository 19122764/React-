def bubble_sort(arr):
    n = len(arr)

    for i in range(n):
        for j in range(0, n-i-1):
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]

# Vetor de exemplo
vetor = [9, 5, 7, 2, 6, 1, 3, 0, 4, 8]

# Chamada da função para ordenar o vetor
bubble_sort(vetor)

# Exibição do vetor ordenado
print("Vetor ordenado:", vetor)
