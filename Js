npm init -y
npm install express
const express = require('express');
const app = express();
const port = 3000;

app.get('/', (req, res) => {
    res.send('Bem-vindo ao meu projeto Node.js!');
});

app.listen(port, () => {
    console.log(`Servidor est√° rodando em http://localhost:${port}`);
});

"scripts": {
    "start": "node index.js"
}
npm start
